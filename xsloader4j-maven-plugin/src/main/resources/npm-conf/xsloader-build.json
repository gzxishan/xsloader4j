{
  "builds": [
    {
      "name": "vue-property-decorator",
      "build": true,
      "product": true,
      "export": "vue-property-decorator",
      "ignoreCurrentRequireDep": true,
      "dependencies": {
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        //设置为true表示安装最新的，并替换成新版本
        "draft-js": true
      },
      "localDependencies": {
        "quill": {
          "version": "2.0.0-dev.3",
          "enable": true,
          "dir": "webapp/local-lib/quill/2.0.0-dev.3/",
          "main": "quill.js",
          "entry": {
            "quill.js": ["./quill.js"],
            "quill.core.js": ["./core.js"],
            "quill.core": "./assets/core.styl",
            "quill.bubble": "./assets/bubble.styl",
            "quill.snow": "./assets/snow.styl"
          }
        }
      },
      "externals": {
        "vue": {
          "commonjs": "vue",
          "commonjs2": "vue",
          "amd": "vue"
        },
        "react": {
          "commonjs": "react",
          "commonjs2": "react",
          "amd": "react"
        },
        "react-dom": {
          "commonjs": "react-dom",
          "commonjs2": "react-dom",
          "amd": "react-dom"
        }
      }
    }
  ]
}